// Page content
#page-content.page-content
  // Contacts Header
  .content-header
    .header-section
      h1
        i.gi.gi-stats
        | Feedback
        br
        small {{one_checkin_feedback[0].checkInId.checkInTime | date:'medium'}}
        button.btn.btn-alt.btn-xs.btn-primary Export to Excel
        button.btn.btn-alt.btn-xs.btn-link.btn-default(disabled, style="border: none;color: #000;") Query by:
        button.btn.btn-alt.btn-xs.btn-default Location
        button.btn.btn-alt.btn-xs.btn-default Time Period
        button.btn.btn-alt.btn-xs.btn-default Activity Type

  // END Contacts Header
  // Main Row
  .row
    .block
      .table-responsive
        table#general-table.table.table-hover.table-vcenter.feedback-grid(ng-repeat="fd in all_feedback")
          thead
            tr
              th.lighter Activity
              th.col-md-3.lighter Status
              th.lighter Feedback Assets
              th.lighter Delay
              //- th.lighter Response
          tbody

            tr
              td
                p(style="margin-bottom: 0;") <strong>CheckIn</strong>
                span at {{fd.locationId.name}}, <span>{{fd.locationId.address}}</span>
              td
                | Active
              td
                .btn-group
                  a.btn.btn-clear.btn-default
                    i.fa.gi(ng-show="fd.decision == 'Yes", style="color: green") Yes
                    i.fa.gi(ng-show="fd.decision == 'No'", style="color: red") No

                  a.btn.btn-clear.btn-default
                      i.fa.fa-comment
                  a.btn.btn-clear.btn-default
                      i.fa.fa-picture-o
                  a.btn.btn-clear.btn-default
                      i.fa.fa-video-camera
              td
                | {{f.answers[0].timeUpdated | date: 'medium'}}

          tfoot
            pagination.pull-right(page-to="turnPage")
            div.clearfix